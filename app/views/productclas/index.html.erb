<%= render 'layerbegin' %>
<div class="col-lg-12">
  <div class="ibox float-e-margins">
    <div class="ibox-title">
      <h3>单位管理</h3>
    </div>
    <div class="ibox-content">
      <table class="table table-hover table-bordered text-center">
        <thead>
        <tr>
          <th class="text-center">单位</th>
          <th class="text-center">操作</th>
        </tr>
        </thead>
        <tbody>
        <% @productclas.each do |productcla| %>
            <tr>
              <td><%= productcla.cla %></td>
              <td><%= link_to '编辑',edit_seller_productcla_path(@seller,productcla) %>
                <%= link_to '删除', [@seller,productcla], method: :delete, data: { confirm: '确认删除?' } %></td>
            </tr>
        <% end %>
        </tbody>
      </table>
      <%= link_to "返回",seller_path(@seller),class: "btn btn-w-m btn-info " %>
      <div class="btn-group pull-right">
        <%= link_to "新增",new_seller_productcla_path,class: "btn btn-w-m btn-primary pull-right" %>
      </div>
      <br>
    </div>
  </div>
</div>
<%=render 'layerend' %>