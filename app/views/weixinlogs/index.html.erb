<%= render 'layerbegin' %>
<div class="col-lg-12">
  <div class="ibox float-e-margins">
    <div class="ibox-title">
      <h3>商家管理</h3>
    </div>
    <div class="ibox-content">
      <table class="table table-hover table-bordered text-center">
        <thead>
        <tr>
          <th class="text-center">开发者微信号</th>
          <th class="text-center">发送方OpenID</th>
          <th class="text-center">消息创建时间</th>
          <th class="text-center">消息类型</th>
          <th class="text-center">事件类型</th>
          <th class="text-center">事件KEY值</th>
          <th class="text-center">二维码ticket</th>
          <th class="text-center">操作</th>
        </tr>
        </thead>
        <tbody>
        <% @weixinlog.each do |weixinlog| %>
            <tr>
              <td><%= weixinlog.ToUserName %></td>
              <td><%= weixinlog.FromUserName %></td>
              <td><%= weixinlog.CreateTime %></td>
              <td><%= weixinlog.MsgType %></td>
              <td><%= weixinlog.Event %></td>
              <td><%= weixinlog.EventKey %></td>
              <td><%= weixinlog.Ticket %></td>
              <td>
                <%= link_to '编辑',edit_weixinlog_path(weixinlog) %>
                <%= link_to '删除', weixinlog, method: :delete, data: { confirm: '确认删除?' } %></td>
            </tr>
        <% end %>
        </tbody>
      </table>
      <div class="btn-group  btn-w-m pull-right" role="group">
        <%= link_to "新增",new_weixinlog_path,class: "btn btn-w-m btn-primary " %>
      </div>
      <br>
    </div>
  </div>
</div>
<%=render 'layerend' %>