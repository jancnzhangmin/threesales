<%= render 'layerbegin' %>
<div class="col-lg-12">
  <div class="ibox float-e-margins">
    <div class="ibox-title">
      <h3>物流单号管理</h3>
    </div>
    <div class="ibox-content">
      <table class="table table-hover table-bordered text-center">
        <thead>
        <tr>
          <th class="text-center">物流公司</th>
          <th class="text-center">操作</th>
        </tr>
        </thead>
        <tbody>
        <% @logisticorders.each do |logist| %>
            <tr>

              <td><%= logist.ordernumber %></td>

              <td>
                <%= link_to '编辑', edit_logisticorder_path(logist) %>
                <%= link_to '删除', logist, method: :delete,
                            data: { confirm: '确认删除?' } %>
              </td>
            </tr>
        <% end %>
        </tbody>
      </table>
      <div class="btn-group  btn-w-m pull-right" role="group">
        <%= link_to "新增",new_logisticorder_path,class: "btn btn-w-m btn-primary " %>
      <%= link_to "返回",logisticorders_path(@logisticorders),class: "btn btn-w-m btn-info " %>
      </div>
      <br>
    </div>
  </div>
</div>
<%=render 'layerend' %>